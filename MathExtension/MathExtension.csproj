<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<VersionPrefix>0.1.0</VersionPrefix>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryType>git</RepositoryType>
    <Authors>ShInUeXx</Authors>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <RootNamespace>ShInUeXx.Numerics</RootNamespace>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <Description>Math extension for C#</Description>
    <RepositoryUrl>https://github.com/shinuexx/MathExtension</RepositoryUrl>
    <PackageTags>math complex numerics biginteger extension</PackageTags>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
